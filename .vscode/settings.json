{
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.sass-cache": true,
    "**/.cache": true,
    "**/*.pyc": true,
    "**/__pycache__/": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "./dist": true,
    "./tmp": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "./dist": true,
    "./tmp": true,
  }
}